{
  "compilerOptions": {
    "target": "es6",
    "moduleResolution": "Node",
    "module": "ES6",
    //"module": "commonjs",
    "noImplicitAny": false,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "outDir": "dist",
    "lib": ["ES6"]
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "dist"]
}
